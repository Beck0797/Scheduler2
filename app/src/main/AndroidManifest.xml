<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scheduler2">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".register_class" android:label="Register Class"></activity>
        <activity android:name=".assignment" android:label="Assigment"> </activity>
        <activity android:name=".attendance" />
        <activity android:name=".schedule" android:label="Time Table"> </activity>
        <activity android:name=".menu" android:theme="@style/AppTheme.NoActionBar"> </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".signIn" android:label="Sign In"></activity>
        <activity
            android:name=".register"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>